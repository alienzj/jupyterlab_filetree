  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.1",
    "@jupyterlab/docmanager": "^1.0.1",
    "@jupyterlab/filebrowser": "^1.0.1",
    "@jupyterlab/services": "^4.0.1",
    "@phosphor/algorithm": "^1.1.3",
    "@types/file-saver": "^2.0.1",
    "@types/jszip": "^3.1.6",
    "file-saver": "^2.0.2",
    "jszip": "^3.2.2"
  },
  "devDependencies": {
    "rimraf": "^2.6.3",
    "typescript": "^3.5.3"
  },
  "jupyterlab": {
    "extension": true
  }
