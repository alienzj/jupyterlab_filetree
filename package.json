{
  "name": "jupyterlab_filetree",
  "version": "0.1.0",
  "description": "File Tree View as side bar tool",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/youngthejames/jupyterlab_filetree",
  "bugs": {
    "url": "https://github.com/youngthejames/jupyterlab_filetree/issues"
  },
  "author": "James Young",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/youngthejames/jupyterlab_filetree.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.19.1",
    "@jupyterlab/docmanager": "^0.19.1",
    "@jupyterlab/filebrowser": "^0.19.3",
    "@jupyterlab/services": "^3.2.1"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.9.2"
  },
  "jupyterlab": {
    "extension": true
  }
}
